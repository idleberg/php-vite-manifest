{
    "name": "idleberg/vite-manifest",
    "description": "A parser for Vite v2 manifest files",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "vite",
        "vitejs",
        "vite manifest"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Idleberg\\ViteManifest\\": "src"
        }
    },
    "authors": [
        {
            "name": "Jan T. Sott",
            "email": "idleberg@users.noreply.github.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "league/uri": "^6.0"
    },
    "require-dev": {
        "codeception/codeception": "^5.0.4",
        "codeception/module-asserts": "^3.0.0",
        "friendsofphp/php-cs-fixer": "^3.8",
        "phpstan/phpstan": "^1.9",
        "icanhazstring/composer-unused": "^0.8.10"
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix ./src",
        "lint": "php -l ./src",
        "test": [
            "vendor/bin/phpstan analyse ./src --memory-limit=512M --level 5",
            "vendor/bin/codecept run"
        ],
        "unused": "vendor/bin/composer-unused"
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
